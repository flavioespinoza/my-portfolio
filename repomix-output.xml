				<div className="flex gap-4 text-sm font-medium">
					{links.map(({ href, label }) => (
						<Link key={href} href={href} className="transition-colors hover:text-primary">
							{label}
						</Link>
					))}
				</div>
			</div>
		</header>
	)
}
